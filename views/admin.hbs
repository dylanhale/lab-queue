<h6>Admin Panel</h6>
<h3>Welcome {{name}}</h3>
<p>Stories?</p>

{{#if queue}}
    <table class="striped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Date</th>
                <th>Status</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            {{#each queue}}
                <tr>
                    <td><a href="/queue/{{_id}}">{{title}}</a></td>
                    <td>{{createdAt}}</td>
                    <td><span class="dash-status">{{status}}</span></td>
                    <td></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{else}}
    <p>You have nothing</p>
{{/if}}